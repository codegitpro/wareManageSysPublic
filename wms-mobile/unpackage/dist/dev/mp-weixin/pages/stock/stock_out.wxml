<view class="content"><view class="scan"><u-search vue-id="4617abe3-1" disabled="{{true}}" search-icon="scan" show-action="{{false}}" placeholder="请扫描出库单二维码" value="{{order.orderNo}}" data-event-opts="{{[['^click',[['scanCode']]],['^input',[['__set_model',['$0','orderNo','$event',[]],['order']]]]]}}" bind:click="__e" bind:input="__e" bind:__l="__l"></u-search><u-button vue-id="4617abe3-2" text="提交" type="primary" customStyle="width:120rpx;height:64rpx;margin-left:10rpx;" data-event-opts="{{[['^click',[['confirm']]]]}}" bind:click="__e" bind:__l="__l"></u-button></view><view class="order"><view>单据类型：</view><view class="label">{{order.orderTypeLabel}}</view><view>当前货位：</view><view class="label">{{selectLocation}}</view></view><view class="detail"><block wx:for="{{order.detailList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['chooseItem',['$0'],[[['order.detailList','',index]]]]]]]}}" class="{{[item.lineNo===selectItem.lineNo?'detail-item active':'detail-item']}}" bindtap="__e"><view class="left">{{item.lineNo+"、"}}</view><view class="right"><view>{{"物料编码："+item.matCode}}</view><view>{{"物料名称："+item.matName}}</view><view>{{"图号："+item.figNum}}</view><view><label class="_span">{{"需领："+item.quantity}}</label><label class="unit _span">{{"已领："+item.receivedQuantity}}</label></view><view>{{"推荐货位："+(item.locationCode?item.locationCode:'')}}</view></view><view class="tag"><block wx:if="{{item.receivedQuantity===0}}"><u-tag vue-id="{{'4617abe3-3-'+index}}" text="待完成" type="error" bind:__l="__l"></u-tag></block><block wx:if="{{item.quantity>item.receivedQuantity&&item.receivedQuantity>0}}"><u-tag vue-id="{{'4617abe3-4-'+index}}" text="领料中" type="warning" bind:__l="__l"></u-tag></block><block wx:if="{{item.quantity===item.receivedQuantity}}"><u-tag vue-id="{{'4617abe3-5-'+index}}" text="已完成" type="success" bind:__l="__l"></u-tag></block></view></view><u-gap vue-id="{{'4617abe3-6-'+index}}" bgColor="#e5e6e7" height="1" bind:__l